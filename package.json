    "aws-cdk": "2.164.1",
    "esbuild": "^0.24.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.164.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1691.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
